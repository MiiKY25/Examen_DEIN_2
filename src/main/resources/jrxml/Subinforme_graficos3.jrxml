	<query language="sql"><![CDATA[SELECT 
    a.nombre AS alumno,
    COUNT(h.id_prestamo) AS total_prestamos
FROM 
    librosexamen2.Historial_prestamo h
INNER JOIN 
    librosexamen2.Estudiante a ON h.dni_estudiante = a.dni_estudiante
GROUP BY 
    a.nombre
ORDER BY 
    total_prestamos DESC

]]></query>
