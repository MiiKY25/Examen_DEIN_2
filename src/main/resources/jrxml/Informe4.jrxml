		<query language="SQL"><![CDATA[SELECT 
    e.dni_estudiante AS dni,
    e.nombre,
    e.primerApellido AS apellido1,
    e.segundApellido AS apellido2,
    COUNT(DISTINCT hp.id_prestamo) AS libros_prestados,
    COALESCE(SUM(DATEDIFF(COALESCE(hp.fecha_devolucion, NOW()), hp.fecha_prestamo)), 0) AS dias_prestamo,
    COUNT(DISTINCT CASE WHEN hp.fecha_devolucion IS NOT NULL THEN hp.id_prestamo END) AS libros_devueltos
FROM 
    librosexamen2.Estudiante e
LEFT JOIN 
    librosexamen2.Historial_prestamo hp ON hp.dni_estudiante = e.dni_estudiante
GROUP BY 
    e.dni_estudiante, e.nombre, e.primerApellido, e.segundApellido

]]></query>
